<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <PackageId>utte.numberset</PackageId>
    <Version>2.0.0</Version>
    <Authors>Claes-Erik Rydberg</Authors>
    <Company></Company>
    <Product>NumberSet</Product>
    <Description>Generic sets for numbers or numberlike objects and Union, Interection and Difference for them. The sets track if they contain their endpoints and thus wether they are Open, Closed or neither.</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Title>NumberSet</Title>
    <Copyright>Claes-Erik Rydberg</Copyright>
    <RepositoryUrl>https://github.com/Claes-ErikR/NumberSet</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>Set, number set, numeric set, sets, number sets, numeric sets</PackageTags>
    <PackageReadmeFile>ReadMe.md</PackageReadmeFile>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\..\..\..\..\..\..\..\..\Temp\New folder\ReadMe.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
